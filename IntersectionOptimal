class Solution:
    def intersect(self, a, b):
        seen = {}
        intersection = []
        for i in range(len(a)):
            seen[a[i]]=True
        for j in range(len(b)):
            if b[j] in seen:
                intersection.append(b[j])
                del seen[b[j]]
        return intersection


# With built in functions
class Solution:
    def intersect(self, a, b):
        intersection = list(set(a) & set(b))
        return intersection 



       
